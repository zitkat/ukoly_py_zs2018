class Moje_cislo(int):

    def je_sude(self):
        print("Já jsem {} a můj typ je {}".format(self, type(self)))
        if self % 2 == 0:
            return True
        else:
            return False

a = Moje_cislo(12)
b = Moje_cislo("13")

print(a, type(a))
print(b, type(b))

print(je_sude(a))
print(b.je_sude())

print(type(a + b))
